<h1>sign up</h1>

<% if flash[:my_error] %>
      <%flash[:my_error].each do |error|%>
         <p><%= error %></p>
        <%end%>
    <%end%>

<%= form_for @camper do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name  %>
    <%= f.label :age %>
    <%= f.number_field :age  %>
   
      
       <%= f.submit %>
   <% end %>